pipeline {
    agent any

    stages {
        stage('one') {
            steps {
                echo 'Building..'
            }
        }
        stage('Two') {
            steps {
                input('Testing..')
            }
        }
        stage('Three') {
            when {
                not {
                    branch "master"
                    }
            }                    
            steps {
                echo 'Deploying....'
            }
        }
        stage('Four') {
            parallel {
                stage('Unit Test') {
                    steps {
                        echo "four"
                    }
                }
                stage('Integration test') {
                    agent {
                        docker {
                            reuseNode false
                            image ;ubuntu'
                        }
                    }
                    steps {
                    echo "running"
                    }
    }
}
