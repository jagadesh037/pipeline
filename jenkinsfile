pipeline {
    agent any

    stages {
        stage('one') {
            steps {
                echo 'Building..'
            }
        }
        stage('Two') {
            steps {
                input('Testing..')
            }
        }
        stage('Three') {
            when {
                not {
                    branch "master"
                    }
            }                    
            steps {
                echo 'Deploying....'
            }
        }  
    }
}
